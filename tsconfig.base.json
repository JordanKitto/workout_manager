{
    "compilerOptions": {
        "target": "ES2021",
        "lib": [
            "ES2021",
            "DOM"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
            "@workout/domain": [
                "packages/domain/src"
            ],
            "@workout/ui": [
                "packages/ui/src"
            ]
        }
    }
}