{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "target": "ES2019",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "rootDir": "src",
        "outDir": "dist",
        "declaration": true,
        "emitDeclarationOnly": false,
        "noEmit": false,
        "skipLibCheck": true
    },
    "include": [
        "src/**/*"
    ]
}